
public class myWhileClass {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 1;
		while(i<=10){
			System.out.println("Loop " + i);
			i++;
		}
		int j = 30;
		do{
			System.out.println("Another loop " +j);
			j--;
		}while(j>=20);
		foo:
			for(int i=1;i<=5;i++)
				for(int j=1;j<=3;j++){
					System.out.println("I is " + i + " , J is " + j);
				}
	}

}
