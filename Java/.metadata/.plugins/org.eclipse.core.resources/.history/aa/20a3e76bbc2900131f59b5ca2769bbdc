
public class myExampleClass {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 5;
		int b = 10;
		float c = 5.6f;
		float d = 13.4f;
		int x, y, z;
		x = y = z = 3;
		x += 3;
		System.out.println("b + a is " + (b+a));
		System.out.println("b - a is " + (b-a));
		System.out.println("b * a is " + (b*a));
		System.out.println("b / a is " + (b/a));
		System.out.println("b % a is " + (b%a));
		System.out.println("d + c is " + (d+c));
		System.out.println("x,y,z is " + x + "," + y + "," + z);
	}

}
