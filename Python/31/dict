def createDict():
    spanish=dict()
    spanish['hello']='hola'
    spanish['yes']='si'
    spanish['one']='uno'
    spanish['two']='dos'
    spanish['three']='tres'
    spanish['red']='rojo'
    spanish['black']='negro'
    return spanish

